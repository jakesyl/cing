<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />
	<!--
		Inherit the default GWT style sheet. Changed to use explicit files
		from gwt for instantiations. <inherits
		name='com.google.gwt.user.theme.standard.Standard' />
	-->
	<inherits name='com.allen_sauer.gwt.voices.gwt-voices' /> <!-- Inherit sound support                        -->
	<inherits name='com.gwtsolutions.components.Components' /> <!-- Inherit message support                        -->
	<inherits name="com.google.gwt.json.JSON" />

	<inherits name='com.google.gwt.i18n.I18N' /> <!--  Language support -->
	<inherits name="com.google.gwt.user.theme.standard.StandardResources" />
	<inherits name="com.google.gwt.user.theme.chrome.ChromeResources" />
	<inherits name="com.google.gwt.user.theme.dark.DarkResources" />

	<entry-point class="cing.client.iCing" />

	<extend-property name="locale" values="en" />
<!-- Disable for quicker testing/compiling.-->
	<extend-property name="locale" values="cn" />
	<extend-property name="locale" values="de" />
	<extend-property name="locale" values="es" />
	<extend-property name="locale" values="fr" />
	<extend-property name="locale" values="it" />
	<extend-property name="locale" values="nl" />
	<extend-property name="locale" values="pt" />

    <!-- Specify the servlet that handles file uploads. 
	<servlet path="/servlets/fileupload" class="cing.server.FileUploadServlet" />	
	-->
</module>