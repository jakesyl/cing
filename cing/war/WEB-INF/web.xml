<?xml version="1.0" encoding="UTF-8"?>
<web-app>

	<servlet>
		<servlet-name>shell</servlet-name>
		<servlet-class>com.google.gwt.dev.shell.HostedMode</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>shell</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>

    <servlet>
      <servlet-name>iCingServlet</servlet-name>
      <servlet-class>
          cing.server.iCingServlet
      </servlet-class>
   </servlet>

    <servlet-mapping>
      <servlet-name>iCingServlet</servlet-name>
      <url-pattern>/iCingServlet</url-pattern>
   </servlet-mapping>

   <welcome-file-list>
      <welcome-file>iCing.html</welcome-file>
   </welcome-file-list>

</web-app>